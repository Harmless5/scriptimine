#!/bin/bash
# Skript, mis k체sib kasutajalt (1-12) kuu numbri ja v채ljastab sellele vastava kuu nime kasutades case k채sku.
echo -n "Sisesta kuu number: "
read kuu
case $kuu in
    "1") echo "Jaanuar";;
    "2") echo "Veebruar";;
    "3") echo "M채rts";;
    "4") echo "Aprill";;
    "5") echo "Mai";;
    "6") echo "Juuni";;
    "7") echo "Juuli";;
    "8") echo "August";;
    "9") echo "September";;
    "10") echo "Oktoober";;
    "11") echo "November";;
    "12") echo "Detsember";;
    *) echo "Sellist kuud ei eksisteeri.";;
esac