#!/bin/bash
# Skript, mis k체sib kasutajalt ridade ja t채rnide arvu ning v채ljastab vastava kujundi.
read -p "Sisesta ridade arv: " read
read -p "Sisesta t채rnide arv: " tarnid
for (( i=1; i<=$read; i++ ))
do
    echo -n "$i. "
    for (( j=1; j<=$tarnid; j++ ))
    do
        echo -n "* "
    done
    echo ""
done